{
  "name": "ai-agent-competition",
  "version": "1.0.0",
  "description": "AI Agent Escape Room Competition Platform",
  "private": true,
  "workspaces": [
    "core",
    "agents",
    "webcli"
  ],
  "scripts": {
    "dev": "npm run dev --workspace=core",
    "dev:webcli": "npm run dev --workspace=webcli",
    "build": "npm run build --workspaces",
    "start": "npm run start --workspace=core",
    "play": "npm run play --workspace=core",
    "play:visual": "npm run play:visual --workspace=core",
    "agent:memoryless": "npm run memoryless --workspace=agents",
    "agent:simple": "npm run simple --workspace=agents",
    "test": "npm run test --workspaces --if-present",
    "lint": "npm run lint --workspaces --if-present",
    "format": "npm run format --workspaces --if-present"
  },
  "keywords": [
    "ai",
    "agent",
    "competition",
    "escape-room"
  ],
  "author": "",
  "license": "MIT"
}
